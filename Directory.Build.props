<Project>

  <PropertyGroup Label="Build">
    <TargetFramework>net7.0</TargetFramework>
    <AssemblyName>Boilerplate.$(MSBuildProjectName)</AssemblyName>
    <RootNamespace>Boilerplate.$(MSBuildProjectName)</RootNamespace>
    <LangVersion>latest</LangVersion>
    <AnalysisLevel>latest</AnalysisLevel>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <OutputPath>$(SolutionDir)artifacts\</OutputPath>
  </PropertyGroup>

  <PropertyGroup Label="Package">
    <Authors>Mike Kozhevnikov</Authors>
    <Company>Mike Kozhevnikov</Company>
    <Copyright>Copyright Â© Mike Kozhevnikov. All rights Reserved</Copyright>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/mkozhevnikov/boilerplate</PackageProjectUrl>
    <RepositoryUrl>https://github.com/mkozhevnikov/boilerplate.git</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageReleaseNotes>https://github.com/mkozhevnikov/boilerplate/releases</PackageReleaseNotes>
  </PropertyGroup>

</Project>
