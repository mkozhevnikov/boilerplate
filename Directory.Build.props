<Project>

  <PropertyGroup Label="Build">
    <LangVersion>latest</LangVersion>
    <AnalysisLevel>latest</AnalysisLevel>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <PropertyGroup Label="Package">
    <Authors>Mike Kozhevnikov</Authors>
    <Company>Mike Kozhevnikov</Company>
    <Copyright>Copyright Â© Mike Kozhevnikov. All rights Reserved</Copyright>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/mkozhevnikov/boilerplate</PackageProjectUrl>
    <RepositoryUrl>https://github.com/mkozhevnikov/boilerplate.git</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageReleaseNotes>https://github.com/mkozhevnikov/boilerplate/releases</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup Label="Package References">
    <PackageReference Include="Microsoft.VisualStudio.Threading.Analyzers" PrivateAssets="all" Version="17.1.46" />
    <PackageReference Include="MinVer" PrivateAssets="all" Version="3.1.0" />
  </ItemGroup>

</Project>
